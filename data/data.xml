<?xml version="1.0" encoding='UTF-8'?>
<odoo>
    <data noupdate="1">
        <!-- sequence master data pasien -->
        <record id="seq_bangunan_area" model="ir.sequence">
            <field name="name">Sequence Bangunan</field>
            <field name="code">bangunan.area.sequence</field>
            <field name="prefix">BA</field>
            <field name="padding">2</field>
        </record>
        <record id="seq_bangunan_zona" model="ir.sequence">
            <field name="name">bangunan zona</field>
            <field name="code">bangunan.zona.sequence</field>
            <field name="prefix">BZ</field>
            <field name="padding">3</field>
        </record>
            <record id="seq_bangunan_bangunan" model="ir.sequence">
            <field name="name">bangunan bangunan</field>
            <field name="code">bangunan.bangunan.sequence</field>
            <field name="prefix">BB</field>
            <field name="padding">3</field>
        </record>
            <record id="seq_bangunan_lantai" model="ir.sequence">
            <field name="name">bangunan lantai</field>
            <field name="code">bangunan.lantai.sequence</field>
            <field name="prefix">BL</field>
            <field name="padding">3</field>
        </record>
            <record id="seq_bangunan_ruang" model="ir.sequence">
            <field name="name">bangunan ruang</field>
            <field name="code">bangunan.ruang.sequence</field>
            <field name="prefix">BR</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>